@model GameViewModel

@{
    ViewBag.Title = Resources.Edit;
}

<div class="container">
    @using (Html.BeginForm())
    {
        <div class="col-md-12">
            <h2>Resources.Edit/h2>

            @Html.AntiForgeryToken()
            <div class="validation-errors">
                @Html.ValidationSummary(false)
            </div>
            @Html.EditorForModel()
        </div>
        <input type="submit" value="@Resources.Edit" />
    }
</div>




